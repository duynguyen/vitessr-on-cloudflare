{
  "scripts": {
    "dev": "npm run server",
    "prod": "npm run build:vite && npm run server:prod",
    "worker": "npm run build && npm run server:worker",
    "build": "npm run build:vite && npm run build:worker",
    "build:vite": "vite build && vite build --ssr",
    "build:worker": "node -r esbuild-register build-worker.js",
    "server": "node ./server",
    "server:prod": "cross-env NODE_ENV=production node ./server",
    "server:worker": "npx wrangler@beta pages dev ./dist/client/"
  },
  "dependencies": {
    "@adobe/aem-core-components-react-base": "^1.1.8",
    "@adobe/aem-core-components-react-spa": "^1.2.0",
    "@adobe/aem-headless-client-js": "^3.1.1",
    "@adobe/aem-react-editable-components": "file:adobe-aem-react-editable-components-1.1.10.tgz",
    "@adobe/aem-spa-component-mapping": "^1.1.1",
    "@adobe/aem-spa-page-model-manager": "^1.4.0",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@vitejs/plugin-react": "^1.2.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.38",
    "esbuild-register": "^3.3.2",
    "express": "^4.17.3",
    "miniflare": "^2.4.0",
    "node-fetch": "^2.6.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "vite": "^2.8.4",
    "vite-plugin-ssr": "^0.3.64",
    "vite-plugin-windicss": "^1.8.4",
    "windicss": "^3.5.1"
  }
}
